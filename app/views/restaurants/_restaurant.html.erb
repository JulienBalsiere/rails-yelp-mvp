<div id="<%= dom_id restaurant %>">
  <p>
    <strong>Name:</strong>
    <%= restaurant.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= restaurant.address %>
  </p>

  <p>
    <strong>Rating:</strong>
    <ul>
      <% restaurant.reviews.each do |review| %>
        <li><%= review.rating %></li>
      <% end %>
    </ul>
  </p>

</div>
